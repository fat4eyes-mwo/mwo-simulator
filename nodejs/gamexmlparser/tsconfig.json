{
    "compilerOptions": {
        "module": "commonjs",
        "allowSyntheticDefaultImports" : true,
        "noImplicitAny": true,
        "strictNullChecks" : false,
        "removeComments": false,
        "preserveConstEnums": true,
        "baseUrl" : "./",
        "sourceMap": true,
        "target": "es6",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./build"
    },
    "files": [
      "src/index.ts"
    ]
}
