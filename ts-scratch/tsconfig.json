{
    "compilerOptions": {
        "module": "System",
        "allowSyntheticDefaultImports" : true,
        "allowJs" : true,
        "noImplicitAny": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "baseUrl" : "./",
        "sourceMap": true,
        "target": "es6",
        "lib" : ["DOM","ES6","DOM.Iterable","ScriptHost","ES2015","ES2016"],
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "outFile" : "./js/out.js"
    },
    "files": [
      "main.ts"
    ]
}
